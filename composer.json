{
    "name": "gefud/gefud",
    "description": "Generator for Entities, Factories and their Usecases in Ddd",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gefud/generator"
        }
    ],
    "require-dev": {
        "mandango/mondator": "dev-master",
	"symfony/console": "2.6.*@dev",
        "phing/phing": "*",
        "phpspec/phpspec": "*",
        "behat/behat": "~2.5",
	"gefud/generator": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Micha≈Ç Brzuchalski",
            "email": "michal.brzuchalski@gmail.com"
        }
    ],
    "keywords": [
        "dto",
        "ddd",
        "domain",
        "driven",
        "design",
        "generator"
    ],
    "autoload": {
        "psr-4": {
            "Gefud\\": "src/"
        }
    },
    "config": {
        "vendor-dir": "vendor/",
        "bin-dir": "bin/"
    },
    "bin": [
        "bin/gefud"
    ]
}
